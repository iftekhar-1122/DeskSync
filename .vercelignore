# Vercel ignore file - exclude files that cause build issues

# Node modules
node_modules/

# Build outputs
dist/
.next/
out/

# Prisma generated files that cause TypeScript issues
packages/database/src/generated/
**/src/generated/

# Test files
**/*.test.ts
**/*.test.tsx
**/*.spec.ts
**/*.spec.tsx
test/
tests/
__tests__/

# Development files
*.log
.env.local
.env.development.local
.env.test.local

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Documentation
docs/
*.md
!README.md

# Scripts that aren't needed for deployment
scripts/
*.bat
*.sh

# Testing and development tools
jest.config.js
**/*.config.js
!next.config.js
!tailwind.config.js
!postcss.config.js

# Lock files (Vercel handles these)
package-lock.json
yarn.lock

# Temporary files
tmp/
temp/
.tmp/
